<% if (title) { -%>
/* title: <%= title %> */
<% } -%>
import renderToApp from '../../renderToApp';
<% if (reducer) { -%>
import reducer from '../../reducer/<%= directory %>/<%= reducer %>';
<% } else { -%>
import getReducer from '../../reducer/getReducer';
const reducer = getReducer();
<% } -%>
import <%= container %> from '../../container/<%= directory %>/<%= container %>';

const render = renderToApp(reducer);

render(<%= container %>);
if (module.hot) {
  module.hot.accept('../../container/<%= directory %>/<%= container %>', () => {
    render(<%= container %>);
  });
}
